{{#each this}}
  
<div class="photo-card gallery">
 <a href="{{this.largeImageURL}}"><img src="{{this.webformatURL}}" alt="{{this.tags}}" loading="lazy" width="400" height="210" /></a>
  <div class="info">
    <p class="info-item">
      <b>Likes:</b> 
      <span>{{this.likes}}</span>
    </p>
    <p class="info-item">
      <b>Views: {{this.views}}</b>
    </p>
    <p class="info-item">
      <b>Comments: {{this.comments}} </b>
    </p>
    <p class="info-item">
      <b>Downloads: {{this.downloads}}</b>
    </p>
  </div>
</div>
{{/each}}